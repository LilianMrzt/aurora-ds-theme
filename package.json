{
  "name": "@aurora-ds/theme",
  "version": "3.1.9",
  "description": "A performant CSS-in-JS theme management library for React",
  "author": "Lilian MARZET <lilian.marzet@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LilianMrzt/Aurora.git"
  },
  "homepage": "https://github.com/LilianMrzt/Aurora#readme",
  "bugs": {
    "url": "https://github.com/LilianMrzt/Aurora/issues"
  },
  "keywords": [
    "react",
    "css-in-js",
    "theme",
    "styling",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "playground": "vite --config playground/vite.config.ts",
    "playground:build": "vite build --config playground/vite.config.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "size": "size-limit",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "validate": "npm run lint && npm run typecheck && npm run test:run",
    "ci": "npm run validate && npm run build && npm run size && npm run audit",
    "prepublishOnly": "npm run ci"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^12.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^12.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB",
      "gzip": true
    },
    {
      "path": "dist/index.cjs",
      "limit": "10 KB",
      "gzip": true
    }
  ],
  "packageManager": "npm@10.9.0"
}
